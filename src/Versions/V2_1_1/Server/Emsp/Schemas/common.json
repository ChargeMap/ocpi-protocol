{
  "title": "V2.1.1\\Emsp\\Common",
  "definitions": {
    "business_details": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1,
          "maxLength": 100
        },
        "website": {
          "type": "string",
          "format": "uri"
        },
        "logo": {
          "$ref": "#/definitions/image"
        }
      },
      "required": [
        "name"
      ],
      "additionalProperties": false
    },
    "connector": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "minLength": 1,
          "maxLength": 36
        },
        "standard": {
          "type": "string",
          "enum": [
            "CHADEMO",
            "DOMESTIC_A",
            "DOMESTIC_B",
            "DOMESTIC_C",
            "DOMESTIC_D",
            "DOMESTIC_E",
            "DOMESTIC_F",
            "DOMESTIC_G",
            "DOMESTIC_H",
            "DOMESTIC_I",
            "DOMESTIC_J",
            "DOMESTIC_K",
            "DOMESTIC_L",
            "IEC_60309_2_single_16",
            "IEC_60309_2_three_16",
            "IEC_60309_2_three_32",
            "IEC_60309_2_three_64",
            "IEC_62196_T1",
            "IEC_62196_T1_COMBO",
            "IEC_62196_T2",
            "IEC_62196_T2_COMBO",
            "IEC_62196_T3A",
            "IEC_62196_T3C",
            "TESLA_R",
            "TESLA_S"
          ]
        },
        "format": {
          "type": "string",
          "enum": [
            "SOCKET",
            "CABLE"
          ]
        },
        "power_type": {
          "type": "string",
          "enum": [
            "AC_1_PHASE",
            "AC_3_PHASE",
            "DC"
          ]
        },
        "voltage": {
          "type": "integer"
        },
        "amperage": {
          "type": "integer"
        },
        "tariff_id": {
          "type": "string",
          "minLength": 1,
          "maxLength": 36
        },
        "terms_and_conditions": {
          "type": [ "string", "null" ],
          "format": "uri"
        },
        "last_updated": {
          "type": "string",
          "pattern": "^([0-9]{4})-(1[0-2]|0[0-9])-([0-2][0-9]|3[0-1])T([0-1][0-9]|2[0-4]):[0-5][0-9]:[0-5][0-9]Z?$"
        }
      },
      "required": [
        "id",
        "standard",
        "format",
        "power_type",
        "voltage",
        "amperage",
        "last_updated"
      ],
      "additionalProperties": false
    },
    "evse": {
      "type": "object",
      "properties": {
        "uid": {
          "type": "string",
          "minLength": 1,
          "maxLength": 39
        },
        "evse_id": {
          "type": "string",
          "minLength": 1,
          "maxLength": 48
        },
        "status": {
          "$ref": "#/definitions/evse_status"
        },
        "status_schedule": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "period_begin": {
                "type": "string"
              },
              "period_end": {
                "type": "string"
              },
              "status": {
                "$ref": "#/definitions/evse_status"
              }
            },
            "required": [
              "period_begin",
              "status"
            ],
            "additionalProperties": false
          }
        },
        "capabilities": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "CHARGING_PROFILE_CAPABLE",
              "CREDIT_CARD_PAYABLE",
              "REMOTE_START_STOP_CAPABLE",
              "RESERVABLE",
              "RFID_READER",
              "UNLOCK_CAPABLE"
            ]
          }
        },
        "connectors": {
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "#/definitions/connector"
          }
        },
        "floor_level": {
          "type": [ "string", "null" ],
          "minLength": 1,
          "maxLength": 4
        },
        "coordinates": {
          "$ref": "#/definitions/geo_location"
        },
        "physical_reference": {
          "type": [ "string", "null" ],
          "minLength": 1,
          "maxLength": 16
        },
        "directions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/display_text"
          }
        },
        "parking_restrictions": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "EV_ONLY",
              "PLUGGED",
              "DISABLED",
              "CUSTOMERS",
              "MOTORCYCLES"
            ]
          }
        },
        "images": {
          "type": [ "array", "null" ],
          "items": {
            "$ref": "#/definitions/image"
          }
        },
        "last_updated": {
          "type": "string",
          "pattern": "^([0-9]{4})-(1[0-2]|0[0-9])-([0-2][0-9]|3[0-1])T([0-1][0-9]|2[0-4]):[0-5][0-9]:[0-5][0-9]Z?$"
        }
      },
      "required": [
        "uid",
        "status",
        "connectors",
        "last_updated"
      ],
      "additionalProperties": false
    },
    "evse_status": {
      "type": "string",
      "enum": [
        "AVAILABLE",
        "BLOCKED",
        "CHARGING",
        "INOPERATIVE",
        "OUTOFORDER",
        "PLANNED",
        "REMOVED",
        "RESERVED",
        "UNKNOWN"
      ]
    },
    "exceptional_period": {
      "type": "object",
      "properties": {
        "period_begin": {
          "type": "string",
          "pattern": "^([0-9]{4})-(1[0-2]|0[0-9])-([0-2][0-9]|3[0-1])T([0-1][0-9]|2[0-4]):[0-5][0-9]:[0-5][0-9]Z?$"
        },
        "period_end": {
          "type": "string",
          "pattern": "^([0-9]{4})-(1[0-2]|0[0-9])-([0-2][0-9]|3[0-1])T([0-1][0-9]|2[0-4]):[0-5][0-9]:[0-5][0-9]Z?$"
        }
      },
      "required": [
        "period_begin",
        "period_end"
      ],
      "additionalProperties": false
    },
    "hours": {
      "type": [ "object", "null" ],
      "properties": {
        "regular_hours": {
          "type": [ "array", "null" ],
          "items": {
            "$ref": "#/definitions/regular_hours"
          }
        },
        "twentyfourseven": {
          "type": "boolean"
        },
        "exceptional_openings": {
          "type": [ "array", "null" ],
          "items": {
            "$ref": "#/definitions/exceptional_period"
          }
        },
        "exceptional_closings": {
          "type": [ "array", "null" ],
          "items": {
            "$ref": "#/definitions/exceptional_period"
          }
        }
      },
      "additionalProperties": false
    },
    "image": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "format": "uri"
        },
        "thumbnail": {
          "type": [ "string", "null" ],
          "format": "uri"
        },
        "category": {
          "type": "string",
          "enum": [
            "CHARGER",
            "ENTRANCE",
            "LOCATION",
            "NETWORK",
            "OPERATOR",
            "OTHER",
            "OWNER"
          ]
        },
        "type": {
          "type": "string",
          "minLength": 1,
          "maxLength": 4
        },
        "width": {
          "type": [ "integer", "null" ]
        },
        "height": {
          "type": [ "integer", "null" ]
        }
      },
      "required": [
        "url",
        "category",
        "type"
      ],
      "additionalProperties": false
    },
    "latitude": {
      "type": "string",
      "minLength": 1,
      "maxLength": 10,
      "pattern": "^-?[0-9]{1,2}\\.[0-9]{6}$"
    },
    "longitude": {
      "type": "string",
      "minLength": 1,
      "maxLength": 11,
      "pattern": "^-?[0-9]{1,3}\\.[0-9]{6}$"
    },
    "geo_location": {
      "type": "object",
      "properties": {
        "latitude": {
          "$ref": "#/definitions/latitude"
        },
        "longitude": {
          "$ref": "#/definitions/longitude"
        }
      },
      "additionalProperties": false
    },
    "display_text": {
      "type": "object",
      "properties": {
        "language": {
          "type": "string",
          "minLength": 2,
          "maxLength": 2,
          "pattern": "^[A-Za-z]{2}$"
        },
        "text": {
          "type": "string",
          "minLength": 1,
          "maxLength": 512
        }
      },
      "required": [
        "language",
        "text"
      ],
      "additionalProperties": false
    },
    "regular_hours": {
      "type": "object",
      "properties": {
        "weekday": {
          "type": "integer",
          "minimum": 0,
          "maximum": 7
        },
        "period_begin": {
          "type": "string",
          "pattern": "[0-2][0-9]:[0-5][0-9]"
        },
        "period_end": {
          "type": "string",
          "pattern": "[0-2][0-9]:[0-5][0-9]"
        }
      },
      "additionalProperties": false
    },
    "related_locations": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "latitude": {
            "$ref": "#/definitions/latitude"
          },
          "longitude": {
            "$ref": "#/definitions/longitude"
          },
          "name": {
            "$ref": "#/definitions/display_text"
          }
        }
      }
    }
  }
}